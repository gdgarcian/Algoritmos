   "source": [
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def fibonacci(n):\n",
    "    \n",
    "    if n <= 1:\n",
    "        return 1\n",
    "    \n",
    "    fib = 1\n",
    "    prevFib = 1\n",
    "    \n",
    "    for i in range(2, n):\n",
    "        temp = fib\n",
    "        fib += prevFib\n",
    "        prevFib = temp\n",
    "    \n",
    "    return fib\n"
   ]
  }
 ],
